{
  "name": "@slate-yjs/react",
  "sideEffects": false,
  "version": "0.0.2",
  "description": "React specific components/utils for slate-yjs.",
  "keywords": [
    "slate",
    "yjs",
    "collaborative",
    "react"
  ],
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.module.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn run microbundle --jsx React.createElement",
    "dev": "yarn run microbundle watch --jsx React.createElement"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BitPhinix/slate-yjs.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@slate-yjs/core": "workspace:^",
    "@types/react": "^17.0.34",
    "microbundle": "^0.14.1",
    "react": "^17.0.2",
    "slate": "~0.70.0",
    "yjs": "^13.5.23"
  },
  "peerDependencies": {
    "@slate-yjs/core": "workspace:^",
    "react": ">=16.8.0",
    "slate": ">=0.70.0",
    "slate-react": ">=0.70.0",
    "yjs": "^13.5.23"
  },
  "bugs": {
    "url": "https://github.com/BitPhinix/slate-yjs/issues"
  },
  "homepage": "https://github.com/BitPhinix/slate-yjs#readme",
  "dependencies": {
    "y-protocols": "^1.0.5"
  }
}
